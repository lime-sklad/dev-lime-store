{% for row in data.table_total  %}
	{% if row.value %}
		<tr class="tfoot_row">
			<th class="tfoot_header">
				<span class="mark mark--total-price total_value_title">{{row.title}} - </span>
			</th>
			<td class="tfoot_data">
				<span class="stock_info_text s_result_sprice total_value_content"> {{ row.value }}</span>
				{% if row.mark %}
				<span class="mark total_value_mark"> {{row.mark}} </span>
				{% elseif row.mark_icon %}
				<span class="mark mrgn-left-5"> <img src="{{row.mark_icon}}" class="mark-icon-manat"> </span>
				{% endif %}
			</td>
		</tr>
	{% endif %}
{% endfor %}