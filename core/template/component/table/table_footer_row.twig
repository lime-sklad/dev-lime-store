{% for row in data.table_total  %}
	<tr class="tfoot_row">
		<th class="tfoot_header">
			<span class="total_value_title">{{row.title}} - </span>
		</th>
		<td class="tfoot_data">
			<span class="s_result_sprice mark mark-text"> {{ row.value }}</span>
			{% if row.mark %}
				<span class="mark mark-text mark-right"> {{row.mark}} </span>
			{% elseif row.mark_icon %}
				<span class="mark mark-icon mark-right"> <img src="{{row.mark_icon}}" class="mark-icon-manat"> </span>
			{% endif %}
		</td>
	</tr>
{% endfor %}